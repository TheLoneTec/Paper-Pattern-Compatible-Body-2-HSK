<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<!-- ONSKIN: Covers most of the upper body. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_ThinOnSkin">
		<defName>THIGAPPE_Alien_NovaThinOnSkin</defName>
		<RaceName>Alien_Nova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- ONSKINFULL: Covers most of the whole body. -->
	
	<!-- Any defs with a parent beginning in ML are subject to a master list of apparel restrictions. 
		 See Defs/General/Apparel.xml for details. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_ThinOnSkinFull">
		<defName>THIGAPPE_Alien_NovaThinOnSkinFull</defName>
		<RaceName>Alien_Nova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<!-- PANTS: Only works if Visible Pants is active. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_ThinPants" MayRequire="XeoNovaDan.VisiblePants">
		<defName>THIGAPPE_Alien_NovaThinPants</defName>
		<RaceName>Alien_Nova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_ThinPantsShort" MayRequire="XeoNovaDan.VisiblePants">
		<defName>THIGAPPE_Alien_NovaThinPantsShort</defName>
		<RaceName>Alien_Nova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- MIDDLE -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_ThinMiddle">
		<defName>THIGAPPE_Alien_NovaThinMiddle</defName>
		<RaceName>Alien_Nova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<!-- SHELL -->	
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_ThinShell">
		<defName>THIGAPPE_Alien_NovaThinShell</defName>
		<RaceName>Alien_Nova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_ThinBodyStrap" MayRequire="Ludeon.RimWorld.Ideology">
		<defName>THIGAPPE_Alien_NovaThinBodyStrap</defName>
		<RaceName>Alien_Nova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_ThinArmor">
		<defName>THIGAPPE_Alien_NovaThinArmor</defName>
		<RaceName>Alien_Nova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- Utility -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_ThinUtility">
		<defName>THIGAPPE_Alien_NovaThinUtility</defName>
		<RaceName>Alien_Nova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	

	<!-- TARGETED -->
	
	<!-- Flak pants don't work with the usual pants def because they're in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_ThinFlakPants">
		<defName>THIGAPPE_Alien_NovaThinFlakPants</defName>
		<RaceName>Alien_Nova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- Trooper armor doesn't work with the usual armor def because it's in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_ThinTrooperArmor" MayRequire="VanillaExpanded.VAPPE">
		<defName>THIGAPPE_Alien_NovaThinTrooperArmor</defName>
		<RaceName>Alien_Nova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
</Defs>