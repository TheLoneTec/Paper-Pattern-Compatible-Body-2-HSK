<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<!-- ONSKIN: Covers most of the upper body. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_HulkOnSkin">
		<defName>GAPPE_NorbalHulkOnSkin</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- ONSKINFULL: Covers most of the whole body. -->
	
	<!-- Any defs with a parent beginning in ML are subject to a master list of apparel restrictions. 
		 See Defs/General/Apparel.xml for details. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_HulkOnSkinFull">
		<defName>GAPPE_NorbalHulkOnSkinFull</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_HulkOnSkinFullAA" MayRequire="Aelanna.EccentricTech.AngelApparel" >
		<defName>GAPPE_NorbalHulkOnSkinFullAA</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- PANTS: Only works if Visible Pants is active. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_HulkPants" MayRequire="XeoNovaDan.VisiblePants">
		<defName>GAPPE_NorbalHulkPants</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_HulkPantsShort" MayRequire="XeoNovaDan.VisiblePants">
		<defName>GAPPE_NorbalHulkPantsShort</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- MIDDLE -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_HulkMiddle">
		<defName>GAPPE_NorbalHulkMiddle</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- SHELL -->	
	
	<!-- OnSkinFull parent for general use, e.g. not apparel locked. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_HulkShell">
		<!-- Default outer. -->
		<defName>GAPPE_NorbalHulkShell</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_HulkBodyStrap" MayRequire="Ludeon.RimWorld.Ideology">
		<!-- Slave body strap fix. -->
		<defName>GAPPE_NorbalHulkBodyStrap</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_HulkPowerarmor">
		<!-- For any armor that should cover the entire body. -->
		<defName>GAPPE_NorbalHulkPowerarmor</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- UTILITY -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_HulkUtility">
		<defName>GAPPE_NorbalHulkUtility</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<!-- TARGETED -->
	
	<!-- Flak pants don't work with the usual pants def because they're in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_HulkFlakPants">
		<defName>GAPPE_NorbalHulkFlakPants</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- Trooper armor doesn't work with the usual armor def because it's in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_HulkTrooperArmor" MayRequire="VanillaExpanded.VAPPE">
		<defName>GAPPE_NorbalHulkTrooperArmor</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
</Defs>