<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<!-- ONSKIN: Covers most of the upper body. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FatOnSkin">
		<defName>THIGAPPE_NorbalFatOnSkin</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- ONSKINFULL: Covers most of the whole body. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FatOnSkinFull">
		<defName>THIGAPPE_NorbalFatOnSkinFull</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FatOnSkinFullAA" MayRequire="Aelanna.EccentricTech.AngelApparel" >
		<defName>THIGAPPE_NorbalFatOnSkinAA</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- PANTS: Only works if Visible Pants is active. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FatPants" MayRequire="XeoNovaDan.VisiblePants">
		<defName>THIGAPPE_NorbalFatPants</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FatPantsShort" MayRequire="XeoNovaDan.VisiblePants">
		<defName>THIGAPPE_NorbalFatShorts</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- MIDDLE -->

	<!-- Middle parent for general use, e.g. not apparel locked. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FatMiddle">
		<defName>THIGAPPE_NorbalFatMiddle</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- SHELL -->	
	
	<!-- OnSkinFull parent for general use, e.g. not apparel locked. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FatShell">
		<defName>THIGAPPE_NorbalFatShell</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FatBodyStrap" MayRequire="Ludeon.RimWorld.Ideology">
		<defName>THIGAPPE_NorbalFatBodyStrap</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FatArmor">
		<defName>THIGAPPE_NorbalFatArmor</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<!-- TARGETED -->
	
	<!-- Flak pants don't work with the usual pants def because they're in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FatFlakPants">
		<defName>THIGAPPE_NorbalFatFlakPants</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- Trooper armor doesn't work with the usual armor def because it's in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FatTrooperArmor" MayRequire="VanillaExpanded.VAPPE">
		<defName>THIGAPPE_NorbalFatTrooperArmor</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FatUtility">
		<defName>THIGAPPE_NorbalFatUtility</defName>
		<RaceName>Norbal</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
</Defs>