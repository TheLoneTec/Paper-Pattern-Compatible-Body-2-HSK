<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<!-- ONSKIN: Covers most of the upper body. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemaleOnSkin">
		<defName>THIGAPPE_Alien_OrassanFemaleOnSkin</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- ONSKINFULL: Covers most of the whole body. -->
	
	<!-- Any defs with a parent beginning in ML are subject to a master list of apparel restrictions. 
		 See Defs/General/Apparel.xml for details. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemaleOnSkinFull">
		<defName>THIGAPPE_Alien_OrassanFemaleOnSkinFull</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<!-- PANTS: Only works if Visible Pants is active. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemalePants" MayRequire="XeoNovaDan.VisiblePants">
		<defName>THIGAPPE_Alien_OrassanFemalePants</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemalePantsShort" MayRequire="XeoNovaDan.VisiblePants">
		<defName>THIGAPPE_Alien_OrassanFemalePantsShort</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- MIDDLE -->

	<!-- Middle parent for general use, e.g. not apparel locked. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemaleMiddle">
		<defName>THIGAPPE_Alien_OrassanFemaleMiddle</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<!-- SHELL -->	
	
	<!-- OnSkinFull parent for general use, e.g. not apparel locked. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemaleShell">
		<!-- Default outer. -->
		<defName>THIGAPPE_Alien_OrassanFemaleShell</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemaleArmor">
		<!-- For any armor that should cover the entire body. -->
		<defName>THIGAPPE_Alien_OrassanFemaleArmor</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<!-- UTILITY -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemaleUtility">
		<defName>THIGAPPE_Alien_OrassanFemaleUtility</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	
	<!-- TARGETED -->
	
	<!-- Flak pants don't work with the usual pants def because they're in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemaleFlakPants">
		<defName>THIGAPPE_Alien_OrassanFemaleFlakPants</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- Trooper armor doesn't work with the usual armor def because it's in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemaleTrooperArmor" MayRequire="VanillaExpanded.VAPPE">
		<defName>THIGAPPE_Alien_OrassanFemaleTrooperArmor</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemaleShoulderpads"  MayRequire="VanillaExpanded.VARME">
		<!-- VAE Shoulderpads fix. -->
		<defName>THIGAPPE_Alien_OrassanFemaleShoulderpads</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemaleTK" MayRequire="VanillaExpanded.VAPPE">
		<!-- VAE tribal kilt fix. -->
		<defName>THIGAPPE_Alien_OrassanFemaleTK</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemaleBodyStrap" MayRequire="Ludeon.RimWorld.Ideology">
		<!-- Slave body strap fix. -->
		<defName>THIGAPPE_Alien_OrassanFemaleBodyStrap</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_FemaleCorset">
		<!-- Corset fix. -->
		<defName>THIGAPPE_Alien_OrassanFemaleCorset</defName>
		<RaceName>Alien_Orassan</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
</Defs>