<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<!-- ONSKIN: Covers most of the upper body. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleOnSkin">
		<defName>GAPPE_RatkinFemaleOnSkin</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- ONSKINFULL: Covers most of the whole body. -->
	
	<!-- Any defs with a parent beginning in ML are subject to a master list of apparel restrictions. 
		 See Defs/General/Apparel.xml for details. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleOnSkinFull">
		<defName>GAPPE_RatkinFemaleOnSkinFull</defName>
		<RaceName>Ratkin</RaceName>
		
		<OffsetNorth>(0.0170,-0.0400)</OffsetNorth>
		<OffsetSouth>(0.0200,0.0200)</OffsetSouth>
		<OffsetEast>(0.0000,-0.0100)</OffsetEast>
		<OffsetWest>(0.0000,0.0100)</OffsetWest>
		<ScaleNorth>1.0000</ScaleNorth>
		<ScaleSouth>1.1000</ScaleSouth>
		<ScaleEast>1.0000</ScaleEast>
		<ScaleWest>1.0000</ScaleWest>
		
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleOnSkinFullAA" MayRequire="Aelanna.EccentricTech.AngelApparel" >
		<defName>GAPPE_RatkinFemaleOnSkinFullAA</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- PANTS: Only works if Visible Pants is active. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemalePants" MayRequire="XeoNovaDan.VisiblePants">
		<defName>GAPPE_RatkinFemalePants</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemalePantsShort" MayRequire="XeoNovaDan.VisiblePants">
		<defName>GAPPE_RatkinFemalePantsShort</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- MIDDLE -->

	<!-- Middle parent for general use, e.g. not apparel locked. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleMiddle">
		<defName>GAPPE_RatkinFemaleMiddle</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
			
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleMiddleAA" MayRequire="Aelanna.EccentricTech.AngelApparel">
		<!-- Angel Apparel middle pieces. There's no reason I should've needed this def or the shell def. But for some reason the alignment of the AA apparel renders off compared to vanilla, e.g. the flak vest?
			 I've contacted the original author of Angel Apparel and confirmed that all of the included pieces are aligned properly. 
			 Apparently, vanilla body offsets were shifted by 2px back in 1.3. The only conclusion I've been able to come to, based on comparing textures, is that the vanilla apparel graphics weren't properly offset. Assuming they were offset at all. -->
		<defName>GAPPE_RatkinFemaleMiddleAA</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- SHELL -->	
	
	<!-- OnSkinFull parent for general use, e.g. not apparel locked. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleShell">
		<!-- Default outer. -->
		<defName>GAPPE_RatkinFemaleShell</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemalePowerarmor">
		<!-- For any armor that should cover the entire body. -->
		<defName>GAPPE_RatkinFemalePowerarmor</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleShellAA" MayRequire="Aelanna.EccentricTech.AngelApparel">
		<!-- Angel Apparel Shell pieces. See RatkinFemaleMiddleAA. --> 
		<defName>GAPPE_RatkinFemaleShellAA</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
			
	<!-- UTILITY -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleUtility">
		<defName>GAPPE_RatkinFemaleUtility</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	
	<!-- TARGETED -->
	
	<!-- Flak pants don't work with the usual pants def because they're in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleFlakPants">
		<defName>GAPPE_RatkinFemaleFlakPants</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- Trooper armor doesn't work with the usual armor def because it's in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleTrooperArmor" MayRequire="VanillaExpanded.VAPPE">
		<defName>GAPPE_RatkinFemaleTrooperArmor</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleShoulderpads"  MayRequire="VanillaExpanded.VARME">
		<!-- VAE Shoulderpads fix. -->
		<defName>GAPPE_RatkinFemaleShoulderpads</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleTK" MayRequire="VanillaExpanded.VAPPE">
		<!-- VAE tribal kilt fix. -->
		<defName>GAPPE_RatkinFemaleTK</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleBodyStrap" MayRequire="Ludeon.RimWorld.Ideology">
		<!-- Slave body strap fix. -->
		<defName>GAPPE_RatkinFemaleBodyStrap</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FemaleCorset">
		<!-- Corset fix. -->
		<defName>GAPPE_RatkinFemaleCorset</defName>
		<RaceName>Ratkin</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
</Defs>