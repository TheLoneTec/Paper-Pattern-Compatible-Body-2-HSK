<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<!-- ONSKIN: Covers most of the upper body. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_MaleOnSkin">
		<defName>THIGAPPE_Alien_DovaMaleOnSkin</defName>
		<RaceName>Alien_Dova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- ONSKINFULL: Covers most of the whole body. -->
	
	<!-- Any defs with a parent beginning in ML are subject to a master list of apparel restrictions. 
		 See Defs/General/Apparel.xml for details. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_MaleOnSkinFull">
		<defName>THIGAPPE_Alien_DovaMaleOnSkinFull</defName>
		<RaceName>Alien_Dova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<!-- PANTS: Only works if Visible Pants is active. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_MalePants" MayRequire="XeoNovaDan.VisiblePants">
		<defName>THIGAPPE_Alien_DovaMalePants</defName>
		<RaceName>Alien_Dova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_MalePantsShort" MayRequire="XeoNovaDan.VisiblePants">
		<defName>THIGAPPE_Alien_DovaMalePantsShort</defName>
		<RaceName>Alien_Dova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- MIDDLE -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_MaleMiddle">
		<defName>THIGAPPE_Alien_DovaMaleMiddle</defName>
		<RaceName>Alien_Dova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<!-- SHELL -->	
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_MaleShell">
		<defName>THIGAPPE_Alien_DovaMaleShell</defName>
		<RaceName>Alien_Dova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_MaleBodyStrap" MayRequire="Ludeon.RimWorld.Ideology">
		<defName>THIGAPPE_Alien_DovaMaleBodyStrap</defName>
		<RaceName>Alien_Dova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_MaleArmor">
		<defName>THIGAPPE_Alien_DovaMaleArmor</defName>
		<RaceName>Alien_Dova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- UTILITY -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_MaleUtility">
		<defName>THIGAPPE_Alien_DovaMaleUtility</defName>
		<RaceName>Alien_Dova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<!-- TARGETED -->
	
	<!-- Flak pants don't work with the usual pants def because they're in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_MaleFlakPants">
		<defName>THIGAPPE_Alien_DovaMaleFlakPants</defName>
		<RaceName>Alien_Dova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- Trooper armor doesn't work with the usual armor def because it's in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_MaleTrooperArmor" MayRequire="VanillaExpanded.VAPPE">
		<defName>THIGAPPE_Alien_DovaMaleTrooperArmor</defName>
		<RaceName>Alien_Dova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="THIGAPPE_HUMAN_MaleTK" MayRequire="VanillaExpanded.VAPPE">
		<defName>THIGAPPE_Alien_DovaMaleTK</defName>
		<RaceName>Alien_Dova</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
</Defs>