<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<!-- ONSKIN: Covers most of the upper body. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinOnSkin">
		<defName>GAPPE_SkynetInfiltratorThinOnSkin</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- ONSKINFULL: Covers most of the whole body. -->
	
	<!-- Any defs with a parent beginning in ML are subject to a master list of apparel restrictions. 
		 See Defs/General/ApparelMasterlists.xml for details. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinOnSkinFull">
		<defName>GAPPE_SkynetInfiltratorThinOnSkinFull</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinOnSkinFullAA" MayRequire="Aelanna.EccentricTech.AngelApparel">
		<defName>GAPPE_SkynetInfiltratorThinOnSkinFullAA</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- PANTS: Only works if Visible Pants is active. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinPants" MayRequire="XeoNovaDan.VisiblePants">
		<defName>GAPPE_SkynetInfiltratorThinPants</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinPantsShort" MayRequire="XeoNovaDan.VisiblePants">
		<defName>GAPPE_SkynetInfiltratorThinPantsShort</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- MIDDLE -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinMiddle">
		<defName>GAPPE_SkynetInfiltratorThinMiddle</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinMiddleAA" MayRequire="Aelanna.EccentricTech.AngelApparel">
		<defName>GAPPE_SkynetInfiltratorThinMiddleAA</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- SHELL -->	
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinShell">
		<defName>GAPPE_SkynetInfiltratorThinShell</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinShellAA" MayRequire="Aelanna.EccentricTech.AngelApparel">
		<defName>GAPPE_SkynetInfiltratorThinShellAA</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinBodyStrap" MayRequire="Ludeon.RimWorld.Ideology">
		<defName>GAPPE_SkynetInfiltratorThinBodyStrap</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinPowerarmor">
		<defName>GAPPE_SkynetInfiltratorThinPowerarmor</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- UTILITY -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinUtility">
		<defName>GAPPE_SkynetInfiltratorThinUtility</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	

	<!-- TARGETED -->
	
	<!-- Flak pants don't work with the usual pants def because they're in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinFlakPants" MayRequire="XeoNovaDan.VisiblePants">
		<defName>GAPPE_SkynetInfiltratorThinFlakPants</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- Trooper armor doesn't work with the usual armor def because it's in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_ThinTrooperArmor" MayRequire="VanillaExpanded.VAPPE">
		<defName>GAPPE_SkynetInfiltratorThinTrooperArmor</defName>
		<RaceName>SkynetInfiltrator</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
</Defs>