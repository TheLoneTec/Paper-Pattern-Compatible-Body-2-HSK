<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<!-- ONSKIN: Covers most of the upper body. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FatOnSkin">
		<defName>GAPPE_SkynetAgentFatOnSkin</defName>
		<RaceName>SkynetAgent</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- ONSKINFULL: Covers most of the whole body. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FatOnSkinFull">
		<defName>GAPPE_SkynetAgentFatOnSkinFull</defName>
		<RaceName>SkynetAgent</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FatOnSkinFullAA" MayRequire="Aelanna.EccentricTech.AngelApparel" >
		<defName>GAPPE_SkynetAgentFatOnSkinAA</defName>
		<RaceName>SkynetAgent</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- PANTS: Only works if Visible Pants is active. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FatPants" MayRequire="XeoNovaDan.VisiblePants">
		<defName>GAPPE_SkynetAgentFatPants</defName>
		<RaceName>SkynetAgent</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FatPantsShort" MayRequire="XeoNovaDan.VisiblePants">
		<defName>GAPPE_SkynetAgentFatShorts</defName>
		<RaceName>SkynetAgent</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- MIDDLE -->

	<!-- Middle parent for general use, e.g. not apparel locked. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FatMiddle">
		<defName>GAPPE_SkynetAgentFatMiddle</defName>
		<RaceName>SkynetAgent</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- SHELL -->	
	
	<!-- OnSkinFull parent for general use, e.g. not apparel locked. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FatShell">
		<defName>GAPPE_SkynetAgentFatShell</defName>
		<RaceName>SkynetAgent</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FatBodyStrap" MayRequire="Ludeon.RimWorld.Ideology">
		<defName>GAPPE_SkynetAgentFatBodyStrap</defName>
		<RaceName>SkynetAgent</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FatPowerarmor">
		<defName>GAPPE_SkynetAgentFatPowerarmor</defName>
		<RaceName>SkynetAgent</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<!-- TARGETED -->
	
	<!-- Flak pants don't work with the usual pants def because they're in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FatFlakPants" MayRequire="XeoNovaDan.VisiblePants">
		<defName>GAPPE_SkynetAgentFatFlakPants</defName>
		<RaceName>SkynetAgent</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- Trooper armor doesn't work with the usual armor def because it's in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FatTrooperArmor" MayRequire="VanillaExpanded.VAPPE">
		<defName>GAPPE_SkynetAgentFatTrooperArmor</defName>
		<RaceName>SkynetAgent</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_FatUtility">
		<defName>GAPPE_SkynetAgentFatUtility</defName>
		<RaceName>SkynetAgent</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
</Defs>