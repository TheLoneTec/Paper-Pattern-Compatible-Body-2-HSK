<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<!-- ONSKIN: Covers most of the upper body. -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MaleOnSkin">
		<defName>GAPPE_SkynetCovertMaleOnSkin</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- ONSKINFULL: Covers most of the whole body. -->
	
	<!-- Any defs with a parent beginning in ML are subject to a master list of apparel restrictions. 
		 See Defs/General/ApparelMasterlists.xml for details. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MaleOnSkinFull">
		<defName>GAPPE_SkynetCovertMaleOnSkinFull</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MaleOnSkinFullAA" MayRequire="Aelanna.EccentricTech.AngelApparel">
		<defName>GAPPE_SkynetCovertMaleOnSkinFullAA</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- PANTS: Only works if Visible Pants is active. -->

	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MalePants" MayRequire="XeoNovaDan.VisiblePants">
		<defName>GAPPE_SkynetCovertMalePants</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MalePantsShort" MayRequire="XeoNovaDan.VisiblePants">
		<defName>GAPPE_SkynetCovertMalePantsShort</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- MIDDLE -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MaleMiddle">
		<defName>GAPPE_SkynetCovertMaleMiddle</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MaleMiddleAA" MayRequire="Aelanna.EccentricTech.AngelApparel">
		<defName>GAPPE_SkynetCovertMaleMiddleAA</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- SHELL -->	
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MaleShell">
		<defName>GAPPE_SkynetCovertMaleShell</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MaleShellAA" MayRequire="Aelanna.EccentricTech.AngelApparel">
		<defName>GAPPE_SkynetCovertMaleShellAA</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MaleBodyStrap" MayRequire="Ludeon.RimWorld.Ideology">
		<defName>GAPPE_SkynetCovertMaleBodyStrap</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MalePowerarmor">
		<defName>GAPPE_SkynetCovertMalePowerarmor</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- UTILITY -->
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MaleUtility">
		<defName>GAPPE_SkynetCovertMaleUtility</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>

	<!-- TARGETED -->
	
	<!-- Flak pants don't work with the usual pants def because they're in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MaleFlakPants" MayRequire="XeoNovaDan.VisiblePants">
		<defName>GAPPE_SkynetCovertMaleFlakPants</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<!-- Trooper armor doesn't work with the usual armor def because it's in the middle layer. -->
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MaleTrooperArmor" MayRequire="VanillaExpanded.VAPPE">
		<defName>GAPPE_SkynetCovertMaleTrooperArmor</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
	<ApparelPaperPattern.ApparelPaperPatternDef ParentName="HUMAN_MaleTK" MayRequire="VanillaExpanded.VAPPE">
		<defName>GAPPE_SkynetCovertMaleTK</defName>
		<RaceName>SkynetCovert</RaceName>
	</ApparelPaperPattern.ApparelPaperPatternDef>
	
</Defs>